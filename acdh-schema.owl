<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
	<!ENTITY foaf "http://xmlns.com/foaf/spec/#" >
	<!ENTITY acdh "https://vocabs.acdh.oeaw.ac.at/#" >
]>

<rdf:RDF xmlns="https://vocabs.acdh.oeaw.ac.at/acdh#"
     xml:base="https://vocabs.acdh.oeaw.ac.at/acdh"
     xmlns:dct="http://purl.org/dc/terms/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:acdh="&acdh;"
     xmlns:foaf="http://xmlns.com/foaf/spec/#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://vocabs.acdh.oeaw.ac.at/acdh">
        <owl:versionInfo xml:lang="en">2016-07-26 (Matej) - removed imports </owl:versionInfo>
        <owl:versionInfo xml:lang="en">2017-09-02 (Matej) - reworked definitions for properties, renamed number of properties for consistency, added domains and ranges and inverse properties ; added class Metadata; 
            removed @acdh:hasRelatedMethod@ and @acdh:hasMethod@ in favour of ObjectProperties: @acdh:appliedMethod@/@acdh:methodAppliedIn@ and DataProperty: @acdh:appliedMethodDescription@
        </owl:versionInfo>
        <owl:versionInfo xml:lang="en">2017-10-04 (Martina) Introduced hasCreatedDate for single creation dates. Introduced hasStartDate and hasEndDate for starting and end dates of Projects. Refined some comments for better understanding.</owl:versionInfo>
        <rdfs:comment xml:lang="en">In definitions of properties, &quot;resource A&quot; refers to the subject resource of the property and &quot;resource B&quot; to the object resource</rdfs:comment>
        <owl:versionInfo xml:lang="en">2017-07-18 (Martina, Peter)- all classes and properties assigned with rdfs:label, put under acdh-namespace, flattened Hierarchy</owl:versionInfo>
        <owl:versionInfo xml:lang="en">2016-07-19 (Go) - initial version </owl:versionInfo>
        <rdfs:comment xml:lang="en">When reusing definitions from other vocabularies (esp. DataCite), the reference is given in brackets: (DataCite: ContactPerson). When the source definition was changed slightly the reference is marked with &quot;cf.&quot;</rdfs:comment>
        <owl:versionInfo xml:lang="en">2017-04-06 (Martina) - added cardinality and some test description</owl:versionInfo>
        <owl:versionInfo xml:lang="en">2017-08-24 (Martina)- added comments to all classes and properties with rdfs:comment. Questionable parts marked with &apos;(??)&apos;. </owl:versionInfo>
        <rdfs:comment xml:lang="en">This is the ontology for ACDH repository.</rdfs:comment>
        <owl:versionInfo>2017-06-14 removed and renamed some Digital/Non-Digital classes. 
 DigitalResource -&gt; Resource
 DigitalCollection -&gt; Collection
 DigitalProject -&gt; removed
 rearranged the class hierarchy</owl:versionInfo>
        <owl:versionInfo xml:lang="en">2017-08-27 (Matej) - reworked definitions for classes</owl:versionInfo>
        <rdfs:label>ACDH Schema</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:label>rdfs:label</rdfs:label>
    </rdf:Description>
    


    <!-- http://www.w3.org/2002/07/owl#inverseOf -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#inverseOf"/>
    


    <!-- http://www.w3.org/2004/02/skos/core#altLabel -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2004/02/skos/core#altLabel"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/contributor">
        <owl:equivalentProperty rdf:resource="&acdh;hasContributor"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/coverage -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/coverage">
        <owl:equivalentProperty rdf:resource="&acdh;hasSpatialCoverage"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/creator">
        <owl:equivalentProperty rdf:resource="&acdh;hasCreator"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/format -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/format">
        <owl:equivalentProperty rdf:resource="&acdh;hasFormat"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/publisher -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/publisher">
        <owl:equivalentProperty rdf:resource="&acdh;hasHosting"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/relation -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/relation">
        <owl:equivalentProperty rdf:resource="&acdh;hasRelatedProject"/>
        <owl:equivalentProperty rdf:resource="&acdh;relation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/rights -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/rights">
        <owl:equivalentProperty rdf:resource="&acdh;hasLicense"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/source -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/source">
        <owl:equivalentProperty rdf:resource="&acdh;hasSource"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/subject -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/subject">
        <owl:equivalentProperty rdf:resource="&acdh;hasSubject"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/dc/elements/1.1/type -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/elements/1.1/type">
        <owl:equivalentProperty rdf:resource="&acdh;hasCategory"/>
    </owl:ObjectProperty>
    


    <!-- &acdh;cites -->

    <owl:ObjectProperty rdf:about="&acdh;cites">
        <rdfs:subPropertyOf rdf:resource="&acdh;relation"/>
        <owl:inverseOf rdf:resource="&acdh;isCitedBy"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Indicates that A refers to B in a citation.</rdfs:comment>
        <rdfs:label xml:lang="en">cites</rdfs:label>
        <skos:altLabel xml:lang="en">Cites</skos:altLabel>
        <skos:altLabel xml:lang="de">Zitiert</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;compiles -->

    <owl:ObjectProperty rdf:about="&acdh;compiles">
        <rdfs:subPropertyOf rdf:resource="&acdh;relation"/>
        <owl:inverseOf rdf:resource="&acdh;isCompiledBy"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Indicates B is the result of a compile or creation event using A.</rdfs:comment>
        <rdfs:label xml:lang="en">compiles</rdfs:label>
        <skos:altLabel xml:lang="en">Compiles</skos:altLabel>
        <skos:altLabel xml:lang="de">Stellt zusammen</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;continues -->

    <owl:ObjectProperty rdf:about="&acdh;continues">
        <rdfs:subPropertyOf rdf:resource="&acdh;relation"/>
        <owl:inverseOf rdf:resource="&acdh;isContinuedBy"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Indicates A is a continuation of the work B.</rdfs:comment>
        <rdfs:label xml:lang="en">continues</rdfs:label>
        <skos:altLabel xml:lang="en">Continues</skos:altLabel>
        <skos:altLabel xml:lang="de">Setzt fort</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;documents -->

    <owl:ObjectProperty rdf:about="&acdh;documents">
        <rdfs:subPropertyOf rdf:resource="&acdh;relation"/>
        <owl:inverseOf rdf:resource="&acdh;isDocumentedBy"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Indicates A is documentation about B.</rdfs:comment>
        <rdfs:label xml:lang="en">documents</rdfs:label>
        <skos:altLabel xml:lang="en">Documents</skos:altLabel>
        <skos:altLabel xml:lang="de">Dokumentiert</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasAccessRestriction -->

    <owl:ObjectProperty rdf:about="&acdh;hasAccessRestriction">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isAccessRestriction"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Denotes if restricted access applies to the Resource (A).</rdfs:comment>
        <rdfs:comment xml:lang="en">[Matej: what is the range of the property? Why is it object property? ??]</rdfs:comment>
        <rdfs:label xml:lang="en">has access restriction</rdfs:label>
        <skos:altLabel xml:lang="en">Has access restriction</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Zugriffsbeschränkung</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasActor -->

    <owl:ObjectProperty rdf:about="&acdh;hasActor">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasCoverage"/>
        <owl:inverseOf rdf:resource="&acdh;isActor"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Any person or institution (B) which the given resource (A) mentions or is about.</rdfs:comment>
        <rdfs:label xml:lang="en">has actor</rdfs:label>
        <skos:altLabel xml:lang="en">Has actor</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Akteur</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasAppliedMethod -->

    <owl:ObjectProperty rdf:about="&acdh;hasAppliedMethod">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isAppliedMethod"/>
        <rdfs:range rdf:resource="&acdh;Concept"/>
        <rdfs:comment xml:lang="en">The methodology (B) applied in creation or curation of the resource or in the project (A) (cf. DataCite: descriptionType methods) </rdfs:comment>
        <rdfs:label xml:lang="en">has applied method</rdfs:label>
        <rdfs:seeAlso>acdh:appliedMethodDescription a owl:DatatypeProperty</rdfs:seeAlso>
        <skos:altLabel xml:lang="de">Angewandte Methode</skos:altLabel>
        <skos:altLabel xml:lang="en">Applied method</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasAuthor -->

    <owl:ObjectProperty rdf:about="&acdh;hasAuthor">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasCreator"/>
        <owl:inverseOf rdf:resource="&acdh;isAuthor"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Author of resource, who will appear in the citation of it.</rdfs:comment>
        <rdfs:comment xml:lang="en">[(??) Has to be distinguished more clearly from creator.]</rdfs:comment>
        <rdfs:label xml:lang="en">has author</rdfs:label>
        <skos:altLabel xml:lang="en">Has author</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Autor</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasCategory -->

    <owl:ObjectProperty rdf:about="&acdh;hasCategory">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isCategory"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Concept"/>
        <rdfs:comment xml:lang="en">Type of resource, e. g. corpus. Choose from list. Can be refined with description, format, extent, etc.</rdfs:comment>
        <rdfs:comment>[just for resource type, with controlled vocabulary]</rdfs:comment>
        <rdfs:label xml:lang="en">has category</rdfs:label>
        <skos:altLabel xml:lang="en">Has category</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Kategorie</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasContact -->

    <owl:ObjectProperty rdf:about="&acdh;hasContact">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isContact"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person or Organisation (B) that can be contacted regarding information about a Resource or a Project (A).</rdfs:comment>
        <rdfs:comment xml:lang="en">Person with knowledge of how to access, troubleshoot, or otherwise field issues related to the resource. (DataCite: ContactPerson)</rdfs:comment>
        <rdfs:label xml:lang="en">has contact</rdfs:label>
        <skos:altLabel xml:lang="en">Has contact</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Kontakt</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasContributor -->

    <owl:ObjectProperty rdf:about="&acdh;hasContributor">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isContributor"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Agent (person, group, organisation) (B) who was actively involved in creating/curating/editing a Resource, a Collection or in a Project (A). Any person or institution making a significant contribution to the development and/or maintenance of the resource, but whose contribution does not “fit” to another already defined role. (cf. DataCite: Other)
This is a generic property. Use one of the more specific sub-properties where possible.</rdfs:comment>
        <rdfs:label xml:lang="en">has contributor</rdfs:label>
        <skos:altLabel xml:lang="en">Has contributor</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Mitarbeiter oder Beitragenden</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasCoverage -->

    <owl:ObjectProperty rdf:about="&acdh;hasCoverage">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isCoverage"/>
        <rdfs:range rdf:resource="&acdh;Concept"/>
        <rdfs:comment xml:lang="en">Generic indication of what is covered/mentioned in the resource  (its content) along various dimensions (subject, space, time). Use the more specific sub-properties where possible.</rdfs:comment>
        <rdfs:label xml:lang="en">has coverage</rdfs:label>
        <owl:inverseOf></owl:inverseOf>
        <skos:altLabel xml:lang="de">Deckt ab</skos:altLabel>
        <skos:altLabel xml:lang="en">Has coverage</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasCreator -->

    <owl:ObjectProperty rdf:about="&acdh;hasCreator">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isCreator"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person (B) responsible for creation of resource (A).</rdfs:comment>
        <rdfs:label xml:lang="en">has creator</rdfs:label>
        <skos:altLabel xml:lang="en">Has creator</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Urheber</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasCurator -->

    <owl:ObjectProperty rdf:about="&acdh;hasCurator">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isCurator"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person (B) tasked with reviewing, enhancing, cleaning, or standardizing data and the associated metadata submitted for storage, use, and maintenance within a data centre or repository. (cf. DataCite: DataCurator)</rdfs:comment>
        <rdfs:label xml:lang="en">has curator</rdfs:label>
        <skos:altLabel xml:lang="en">Has data curator</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Datenkurator</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasDepositor -->

    <owl:ObjectProperty rdf:about="&acdh;hasDepositor">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isDepositor"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person or organisation (B) responsible for submitting a resource (A) for hosting to the repository.</rdfs:comment>
        <rdfs:label xml:lang="en">has depositor</rdfs:label>
        <skos:altLabel xml:lang="en">Has depositor</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Datengeber</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasDerivedPublication -->

    <owl:ObjectProperty rdf:about="&acdh;hasDerivedPublication">
        <rdfs:subPropertyOf rdf:resource="&acdh;relation"/>
        <owl:inverseOf rdf:resource="&acdh;isDerivedPublication"/>
        <rdfs:range rdf:resource="&acdh;Publication"/>
        <rdfs:comment xml:lang="en">A publication derived from a project, collection or even resource.</rdfs:comment>
        <rdfs:label xml:lang="en">has derived publication</rdfs:label>
        <skos:altLabel xml:lang="en">Has derived publication</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat abgeleitete Publikation</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasEditor -->

    <owl:ObjectProperty rdf:about="&acdh;hasEditor">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isEditor"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">A person who oversees the details related to the publication format of the resource. (DataCite Editor)</rdfs:comment>
        <rdfs:label xml:lang="en">has editor</rdfs:label>
        <skos:altLabel xml:lang="en">Has editor</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Herausgeber</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasFormat -->

    <owl:ObjectProperty rdf:about="&acdh;hasFormat">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isFormat"/>
        <rdfs:domain rdf:resource="&acdh;Resource"/>
        <rdfs:comment xml:lang="en">Format of a resource (A). Indicated as MIME type.</rdfs:comment>
        <rdfs:comment xml:lang="en">[Matej: What is the range of the property? The MIME types, but how do you refer to these. Shouldn&apos;t this be a DatatypeProperty with a controlled vocabulary ??]</rdfs:comment>
        <rdfs:label xml:lang="en">has format</rdfs:label>
        <skos:altLabel xml:lang="en">Has format</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Format</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasFunder -->

    <owl:ObjectProperty rdf:about="&acdh;hasFunder">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isFunder"/>
        <rdfs:domain rdf:resource="&acdh;Project"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person or Organisation (B) which provided funding for the project (A). </rdfs:comment>
        <rdfs:comment xml:lang="en">Person or organisation that issued a contract or under the auspices of which a work has been written, printed, published, developed, etc. (DataCite: Sponsor)</rdfs:comment>
        <rdfs:label xml:lang="en">has funder</rdfs:label>
        <skos:altLabel xml:lang="en">Has funder</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Geldgeber</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasHosting -->

    <owl:ObjectProperty rdf:about="&acdh;hasHosting">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isHosting"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Institution responsible for preservation/hosting of a resource. (DataCite: HostingInstitution) Typically, the organisation allowing the resource to be available on the internet through the provision of its hardware/software/operating support.</rdfs:comment>
        <rdfs:label xml:lang="en">has hosting</rdfs:label>
        <skos:altLabel xml:lang="en">Has host</skos:altLabel>
        <skos:altLabel xml:lang="de">Wird erhalten/bereitgestellt von</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasLicense -->

    <owl:ObjectProperty rdf:about="&acdh;hasLicense">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isLicense"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:comment xml:lang="en">Denotes the license attached to an object.</rdfs:comment>
        <rdfs:label xml:lang="en">has license</rdfs:label>
        <skos:altLabel xml:lang="en">Has license</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Lizenz</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasLicensor -->

    <owl:ObjectProperty rdf:about="&acdh;hasLicensor">
        <rdfs:subPropertyOf rdf:resource="&acdh;hasContributor"/>
        <owl:inverseOf rdf:resource="&acdh;isLicensor"/>
        <rdfs:domain rdf:resource="&acdh;RepoObject"/>
        <rdfs:range rdf:resource="&acdh;Agent"/>
        <rdfs:comment xml:lang="en">Person or Organisation responsible for choosing a license for the Object.</rdfs:comment>
        <rdfs:label xml:lang="en">has licensor</rdfs:label>
        <skos:altLabel xml:lang="en">Has licensor</skos:altLabel>
        <skos:altLabel xml:lang="de">Hat Lizenzgeber</skos:altLabel>
    </owl:ObjectProperty>
    


    <!-- &acdh;hasLifeCycleStatus -->

    <owl:ObjectProperty rdf:about="&acdh;hasLifeCycleStatus">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <owl:inverseOf rdf:resource="&acdh;isLifeCycleStatus"/>
        <rdfs:comment xml:lang="en">Indication if the Project, Collection or Resource (A) still in the making or completed? A verbose status description can be added with acdh:hasCompleteness.</rdfs:comment>
        <rdfs:comment xml:lang="en">[Matej: define allowed values as controlled vocabulary. !!]</rdfs:comment>
